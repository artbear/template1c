			Партия.Дата = Дата;
			Партия.УстановитьНовыйНомер();
			Партия.ВалютаДокумента = Руб;
			Партия.Записать();
		КонецЕсли; 
	КонецЕсли;
	СтруктураСубконтоПартии.Вставить(""Значение"",Партия.Ссылка); 
	Возврат СтруктураСубконтоПартии
	
